<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.briangorski.calendar.model.abstracts.NotificationType" table="notification_type" discriminator-value="NT">
		<id name="notificationID" type="java.lang.Long">
			<column name="NOTIFICATION_ID" unique="true"/>
			<generator class="assigned" />
		</id>
		
		<discriminator column="DISCRIMINATOR" type="string" />
		
		<subclass name="com.briangorski.calendar.model.EmailNotificationType" extends="NotificationType" discriminator-value="ENT">
        
        </subclass>
	</class>
</hibernate-mapping>